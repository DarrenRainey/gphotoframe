from twisted.internet import gtk2reactor
gtk2reactor.install()
from twisted.internet import reactor

try:
    from lib.photoliststore import PhotoListStore
except:
    from gphotoframe.photoliststore import PhotoListStore

if __name__ == "__main__":
    PhotoListStore()
    reactor.run()
